configuration {
    modi: "drun,filebrowser,run,window";
	font: "Fira Code 12";
    show-icons: true;
    icon-theme:	"ePapirus-Dark";
  timeout {
      action: "kb-cancel";
      delay:  0;
  }
  filebrowser {
      directories-first: true;
      sorting-method:    "name";
  }
}
@theme "/usr/share/rofi/themes/android_notification.rasi"
